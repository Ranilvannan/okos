<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="blog_blog_sequence" model="ir.sequence">
            <field name="name">Blog</field>
            <field name="code">blog.blog</field>
            <field name="prefix">Blog/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="blog_author_sequence" model="ir.sequence">
            <field name="name">Blog</field>
            <field name="code">blog.blog</field>
            <field name="prefix">Author/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="blog_template_sequence" model="ir.sequence">
            <field name="name">Blog Template</field>
            <field name="code">blog.template</field>
            <field name="prefix">Blog/Template/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

    </data>
</odoo>